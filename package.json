    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "react-icons": "5.3.0",
    "react-router-dom": "6.26.2",
    "recharts": "2.12.7",
    "styled-components": "6.1.13"
